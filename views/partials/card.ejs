<% 
    const {
        url = `/images/default-album-banner.png`,
        width = 300,
        height = 300
    } = locals.image ?? {};
%>

<div class="card <%=locals.variant%>" title="<%=locals.title%>" data-ripple>
    <div class="card-banner">
        <figure class="img-holder">
            <img src="<%=url%>" width="<%=width%>" height="<%=height%>" alt="<%=locals.title%>" class="img-cover" data-image-load-anim>
        </figure>
    </div>
</div>